<div class="container">
  <div class="row">
    <div class="col">
      <h1>Dashboard</h1>
      <% if current_user.tfa.setup? %>
        <%= link_to "TFA", edit_tfa_path %>
      <% else %>
        <%= link_to "Setup TFA", new_tfa_path %>
      <% end %>
      <table class="table">
        <thead>
          <th>Entity ID</th>
        </thead>
        <tbody>
        <% @metadatum.each do |metadata| %>
          <tr>
            <td><%= metadata.entity_id %></td>
          </tr>
        <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
